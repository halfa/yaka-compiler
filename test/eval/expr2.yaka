PROGRAMME expr2
CONST 
	aa=10,
	ba=VRAI,
	cc=aa;
	
VAR ENTIER c1,c2,c3,c4,c5;
VAR BOOLEEN b1,b2;
	ECRIRE("c1=");
	LIRE(c1);
	ALALIGNE;
	ECRIRE("c2=");
	LIRE(c2);
	ALALIGNE;
 	ECRIRE((aa+cc/2)/5);
 	ALALIGNE;
 	ECRIRE(c1+3*c1-aa);
 	ALALIGNE;
 	ECRIRE(ba OU VRAI);
 	ALALIGNE;
 	ECRIRE(c1<=(c2+4));
 	ALALIGNE;
FPROGRAMME
